def error = null
currentBuild.result = "SUCCESS"


try {
    stage('Checkout code') {
      node('master') {
         checkout scm
       }
    }
    stage('beginning') {
      node('master') {
         echo "hello world"
	 sh "pwd"
         sh "ls -l"
      }
    }
} catch (caughtException) {
    error = caughtException
    currentBuild.result = "FAILURE"
}

